{
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "script"
    },
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "plugins": ["jest"],
    "rules": {
        "no-console": "warn",
        "prettier/prettier": "error",
        "strict": [2, "global"],
        "camelcase": [2, { "ignoreDestructuring": true, "properties": "never" }]
    }
}
